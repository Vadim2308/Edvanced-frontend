{
  "name": "edvanced-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:vite": "vite",
    "start:dev": "concurrently \"npm run start\" \"npm run start:dev:server\"",
    "start:dev:server": "node ./json-server/index.js",
    "generate:slice": "node ./scripts/createSlice/index.js",
    "build:prod": "webpack --env mode=production apiUrl=https://edvanced-frontend-server.vercel.app/",
    "build:dev": "webpack --env mode=development",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "prettier:fix": "prettier . --write",
    "test:e2e": "cypress open",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:DEPRECATED": "npm run storybook:build && loki --requireReference --reactUri file:./storybook-static",
    "test:ui:ci": "concurrently --kill-others --raw --success first \"npm run storybook\" \"npm run test:ui\" \"deelay\"",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "storybook": "storybook dev --no-open -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "postinstall": "rimraf node_modules/.cache"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "eslint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-webpack5": "^7.6.7",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/circular-dependency-plugin": "^5.0.8",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-virtualized": "^9.21.29",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.6.3",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-loader": "^9.1.3",
    "circular-dependency-plugin": "^5.2.2",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.1",
    "css-loader": "^6.9.0",
    "cypress": "^13.6.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-study-type-checker": "^0.0.8",
    "eslint-plugin-unused-imports": "^3.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "json-server": "^0.17.4",
    "loki": "^0.33.1",
    "mini-css-extract-plugin": "^2.7.7",
    "prettier": "3.1.1",
    "react-refresh": "^0.14.0",
    "reg-cli": "^0.17.7",
    "sass": "^1.69.7",
    "sass-loader": "^13.3.3",
    "storybook": "^7.6.7",
    "storybook-addon-mock": "^4.3.0",
    "style-loader": "^3.3.4",
    "stylelint": "^16.1.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "ts-loader": "^9.5.1",
    "ts-morph": "^21.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-svgr": "^4.2.0",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@react-spring/web": "^9.7.3",
    "@reduxjs/toolkit": "^2.0.1",
    "@use-gesture/react": "^10.3.0",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "i18next": "^23.7.16",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.2",
    "react-virtualized": "^9.22.5"
  },
  "overrides": {
    "loki": {
      "@storybook/react": "$@storybook/react"
    }
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  }
}
